
@model IEnumerable<coreDemo.Models.Movie>


    <div class="container">
        <table class="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Name</th>
                    <th scope="col">CinemaId</th>
                    <th scope="col">Starring</th>
                    <th scope="col">ReleaseDate</th>
                    <th scope="col">操作</th>

                </tr>
            </thead>
            <tbody>
                @Html.DisplayForModel()
            </tbody>
        </table>
        <a asp-action="Add" asp-route-cinemaId="@ViewBag.CinemaId">添加</a>
        <a asp-controller="Home" asp-action="Index" >返回</a>

    </div>

